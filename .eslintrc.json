{
  "extends": "airbnb",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "settings": {
    "import/core-modules": ["common", "actions", "constants", "utils", "components", "static"]
  },
  "rules": {
    "max-len": ["error", 120, 4, { "comments": 80 }],
    "indent": ["error", 4],
    "react/jsx-indent": [2, 4],
    "no-unused-vars": ["error", {"vars": "all", "args": "none"}],
    "no-param-reassign": ["error", { "props": false }]
  },
  "plugins": [
    "react"
  ]
}